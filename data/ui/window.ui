<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.94.1 -->
<interface>
  <!-- interface-name window.ui -->
  <requires lib="Adw" version="1.0"/>
  <requires lib="gio" version="2.0"/>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.4"/>
  <template class="GconvertWindow" parent="AdwApplicationWindow">
    <property name="content">
      <object class="AdwToolbarView" id="toolbar_view">
        <property name="content">
          <object class="GtkLabel" id="label">
            <property name="label">Hello, World!</property>
            <style>
              <class name="title-1"/>
            </style>
          </object>
        </property>
        <child>
          <object class="GtkViewport">
            <property name="scroll-to-focus">1</property>
          </object>
        </child>
        <child>
          <object class="AdwViewStack" id="stack">
            <child>
              <object class="AdwViewStackPage">
                <property name="child">
                  <object class="GtkScrolledWindow">
                    <child>
                      <object class="GtkBox" id="main_contain">
                        <property name="margin-bottom">30</property>
                        <property name="margin-end">10</property>
                        <property name="margin-start">10</property>
                        <property name="margin-top">30</property>
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkPicture" id="logo">
                            <property name="alternative-text">logo</property>
                            <property name="file">resource:///com/qsk/gconvert/com.qsk.gconvert.svg</property>
                            <property name="halign">center</property>
                            <property name="keep-aspect-ratio">true</property>
                            <property name="valign">center</property>
                            <style>
                              <class name="logo"/>
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel">
                            <property name="halign">center</property>
                            <property name="label">Add files for conversion</property>
                            <property name="margin-bottom">12</property>
                            <property name="xalign">0</property>
                            <style>
                              <class name="title-1"/>
                            </style>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="convert_btn">
                            <property name="halign">center</property>
                            <property name="label">Convert</property>
                            <property name="sensitive">False</property>
                            <signal name="clicked" handler="convert"/>
                            <style>
                              <class name="suggested-action"/>
                              <class name="next-btn"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
                <property name="name">page1</property>
                <property name="visible">False</property>
              </object>
            </child>
            <child>
              <object class="AdwViewStackPage">
                <property name="child">
                  <object class="GtkBox">
                    <property name="margin-bottom">30</property>
                    <property name="margin-end">10</property>
                    <property name="margin-start">10</property>
                    <property name="margin-top">30</property>
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkLabel">
                        <property name="label">Group 2</property>
                        <property name="margin-bottom">10</property>
                        <property name="margin-top">30</property>
                        <property name="xalign">0</property>
                        <style>
                          <class name="title-2"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="GtkListBox" id="convert_listbox">
                        <property name="selection-mode">none</property>
                        <style>
                          <class name="rich-list"/>
                          <class name="boxed-list"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton" id="next2">
                        <property name="halign">center</property>
                        <property name="label">next</property>
                      </object>
                    </child>
                  </object>
                </property>
                <property name="name">page2</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
    <property name="default-height">600</property>
    <property name="default-width">500</property>
  </template>
</interface>
